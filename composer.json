{
    "name": "sakoora0x/laravel-ethereum-module",
    "description": "Laravel Ethereum Module",
    "homepage": "https://github.com/sakoora0x/laravel-ethereum-module",
    "license": "MIT",
    "authors": [
        {
            "name": "sakoora0x",
            "email": "sakoora0x@gmail.com"
        },
        {
            "name": "MollSoft",
            "email": "info@mollsoft.com"
        }
    ],
    "keywords": [
        "sakoora0x",
        "laravel",
        "ethereum"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92.0",
        "brick/math": "*",
        "sakoora0x/bip39-mnemonic-php": "^1.0.0",
        "sakoora0x/php-bip-44": "^1.0.0",
        "kornrunner/ethereum-address": "^0.3.0",
        "kornrunner/ethereum-offline-raw-tx": "^0.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "sakoora0x\\LaravelEthereumModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sakoora0x\\LaravelEthereumModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sakoora0x\\LaravelEthereumModule\\EthereumServiceProvider"
            ],
            "aliases": {
                "Ethereum": "sakoora0x\\LaravelEthereumModule\\Facades\\Ethereum"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
